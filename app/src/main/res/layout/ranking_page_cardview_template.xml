<?xml version="1.0" encoding="utf-8"?>
<com.non_android_programmers.responsiveguiapi9.ResponsiveCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ranking_page_cardview_template"
    android:layout_width="match_parent"
    android:layout_height="80dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="5dp"
    android:background="@drawable/rounded_layout_shape">


    <com.non_android_programmers.responsiveguiapi9.ResponsiveCardView
        android:id="@+id/ranking_page_inner_cardview_template"
        android:layout_width="match_parent"
        android:layout_height="66dp"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="80dp"
        android:background="@color/colorA17"
        android:elevation="0dp"
        app:cardCornerRadius="10dp">

        <com.q20.projectsummer.Custom.AutoResizeTextViewWithIrsansFont
            android:id="@+id/ranking_page_inner_cardview_text"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:maxLines="1"
            android:text=""
            android:textSize="@dimen/total_recycler_text_size"
            android:layout_marginRight="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="3dp"
            android:gravity="center"
            android:layout_gravity="center"/>
    </com.non_android_programmers.responsiveguiapi9.ResponsiveCardView>

    <com.non_android_programmers.responsivegui.ResponsiveRelativeLayout
        android:layout_width="72dp"
        android:layout_height="72dp"
        android:background="@drawable/register_image_veiw_shape"
        android:layout_marginLeft="297dp"
        android:layout_marginTop="3dp"
        android:gravity="center">

        <com.non_android_programmers.responsivegui.ResponsiveImageView
            android:id="@+id/ranking_page_inner_cardview_image"
            android:layout_width="57dp"
            android:layout_height="57dp"
            android:layout_marginTop="3dp"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="3dp"
            android:layout_marginBottom="3dp"/>
    </com.non_android_programmers.responsivegui.ResponsiveRelativeLayout>

</com.non_android_programmers.responsiveguiapi9.ResponsiveCardView>