<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="10dp"
    android:orientation="vertical">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1.0"
        android:padding="10dp"
        android:layout_margin="10dp"

        android:columnCount="3"
        android:rowCount="4">

        <RelativeLayout
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_column="0"
            android:layout_row="0"
            android:padding="@dimen/profile_image_layouts_padding_size_layout_large"
            android:layout_marginRight="@dimen/profile_image_images_right_margins_size_layout_large"
            android:background="@drawable/profile_image_images_layout_shape">
            <ImageView
                android:id="@+id/profile_image_page_image1"
                android:layout_height="@dimen/profile_image_page_images_size_layout_large"
                android:layout_width="@dimen/profile_image_page_images_size_layout_large"
                android:layout_margin="6dp"
                android:layout_gravity="center"
                android:layout_columnWeight="1.0"
                android:layout_rowWeight="1.0"/>
        </RelativeLayout>

        <RelativeLayout
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:layout_column="1"
            android:layout_row="0"
